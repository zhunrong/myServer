<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no,maximum-scale=1.0">
    <meta http-equiv=" X-UA-Compatible " content="ie=edge ">
    <title>文件管理器</title>
    <link rel="stylesheet" href="/static/explorer/reset.css">
    <link rel="stylesheet" href="/static/explorer/index.css">
</head>

<body>
    <ul class="file-list">
        <% fileArray.forEach(file=>{ %>
            <li>
                <a href="/explorer/<%= file.name %>">
                    <p>
                        <%= file.name %>
                    </p>
                    <p>
                        <%= file.type %>
                    </p>
                </a>
            </li>
            <% }) %>
    </ul>
</body>

</html>